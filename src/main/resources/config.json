{
  "logging": {
    "level": "INFO",
    "directory": "varys.log"
  },
  "git": {
    "parent_directory": "/home/git"
  },
  "modules": [
    {
      "name": "jenkins",
      "enabled": true,
      "config": {
        "jenkins_api": {
          "base_url": "https://jenkins.mono-project.com",
          "api_token": "<your_api_token>"
        },
        "notifications": {
          "period": 30,
          "tray_icon_url": "https://wiki.jenkins.io/download/attachments/2916393/headshot.png",
          "filters": {
            "local_branches_only": true,
            "successful_builds": false
          }
        }
      }
    },
    {
      "name": "gitlab",
      "enabled": true,
      "config": {
        "gitlab_api": {
          "version": 4,
          "base_url": "https://gitlab.com",
          "private_token": "<your_private_token>"
        },
        "notifications": {
          "period": 30,
          "tray_icon_url": "https://gitlab.com/gitlab-com/gitlab-artwork/raw/master/logo/logo.png"
        }
      }
    }
  ]
}